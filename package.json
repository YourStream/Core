{
  "name": "@yourstream/core",
  "version": "0.0.32",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js", 
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/src/*",
      "require": "./dist/src/*"
    },
    "./utils": {
      "import": "./dist/src/utils/index.js",
      "require": "./dist/src/utils/index.cjs"
    },
    "./utils/*": {
      "import": "./dist/src/utils/*",
      "require": "./dist/src/utils/*"
    },
    "./services": {
      "import": "./dist/src/services/index.js",
      "require": "./dist/src/services/index.cjs"
    },
    "./services/*": {
      "import": "./dist/src/services/*",
      "require": "./dist/src/services/*"
    },
    "./types": {
      "import": "./dist/src/types/index.js",
      "require": "./dist/src/types/index.cjs"
    },
    "./types/*": {
      "import": "./dist/src/types/*",
      "require": "./dist/src/types/*"
    },
    "./models": {
      "import": "./dist/src/models/index.js",
      "require": "./dist/src/models/index.cjs"
    },
    "./models/*": {
      "import": "./dist/src/models/*",
      "require": "./dist/src/models/*"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YourStream/Core.git"
  },
  "scripts": {
    "build": "tsc",
    "build-publish": "npm run build && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "VoDACode",
  "license": "ISC",
  "dependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "amqplib": "^0.10.7",
    "generic-pool": "^3.9.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.6.0",
    "redis": "^5.0.0"
  },
  "bugs": {
    "url": "https://github.com/YourStream/Core/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/YourStream/Core#readme",
  "description": "",
  "devDependencies": {
    "@types/redis": "^4.0.11"
  }
}
